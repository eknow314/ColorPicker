<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--HSV矩形颜色选择器-->
    <declare-styleable name="HSVColorPickerView">
        <attr name="hcpv_pointerRes" format="reference" />
        <attr name="hcpv_radius" format="dimension" />
        <attr name="hcpv_brightnessMode" format="boolean" />
    </declare-styleable>
    <!--冷暖色选择器-->
    <declare-styleable name="CWColorPickerView">
        <attr name="ccpv_pointerRes" format="reference" />
        <attr name="ccpv_radius" format="dimension" />
        <attr name="ccpv_startColor" format="color"/>
        <attr name="ccpv_centerColor" format="color"/>
        <attr name="ccpv_endColor" format="color"/>
    </declare-styleable>
</resources>